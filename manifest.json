{
  "manifest_version": 2,
  "name": "My first Chrome Extension",
  "description": "To learn something new and interesting",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },      
  "browser_action": {
    //"default_icon": "icon.png",
    "default_popup": "popup.html"
  },
      
  "permissions": [
    "http://*/*", "https://*/*", "tabs"
  ]
}